<div class="alert alert-success" role="alert">
  <p class="notice"> <%= "「#{params[:q][:id_eq]} #{ params[:q][:nickname_cont]} #{params[:q][:email_cont]} 」の検索結果: #{@results.count}個" %></p>
</div>

<%= paginate @results %>
<table border="1">
    <tr>
      <th>id</th>
      <th>nickname</th>
      <th>email</th>
    </tr>

    <% @results.each do |result| %>
      <tr>
        <td><%= result.id %></td>
        <td><%= result.nickname %></td>
        <td><%= result.email %></td>
      </tr>
    <% end %>
</table>
<br>
<%= paginate @results %>

<%= link_to 'member一覧', staffs_members_path %>